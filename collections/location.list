{@|apply navi-location.block}
<article class="article">
    {@|apply header-banner.block}
    <nav class="crumb meta -wrap -text--left -grey">
        <div class="crumb__li">
            <a href="/locations/">Locations</a>
        </div>
        <div class="crumb__li">
            {collection.title}
        </div>
    </nav>
    <section class="cms -exp-2">
        <div class="article__body">
            <squarespace:block-field id="{collection.urlId}-blocks" columns="12" />
        </div>
    </section>
</article>
