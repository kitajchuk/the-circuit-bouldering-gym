<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1" />
        <link href="https://fonts.googleapis.com/css?family=Roboto+Slab:400,700,300,100|Montserrat:400,700" rel="stylesheet" />
        {squarespace-headers}
    </head>
    <body class="-fzero">
        <squarespace:navigation navigationId="navi" template="navi" />
        {@|apply header.block}
        <main class="main js-main" {.if collection.homepage}data-homepage="true"{.end} data-content-field="main-content" data-website-id="{website.id}" {.section collection}data-collection-id="{collection.id}" data-collection-title="{.if collection.homepage}{website.siteTitle}{.or}{collection.title} – {website.siteTitle}{.end}"{.end} {.section item}data-item-id="{item.id}" data-item-title="{item.title} – {collection.title} – {website.siteTitle}"{.end}>
            {.if collection.homepage}
                {@|apply home.block}
            {.or}
                {.equal? collection.typeName "page"}
                    {@|apply page.block}
                {.or}
                    {squarespace.main-content}
                {.end}
            {.end}
        </main>
        {@|apply footer.block}
        {@|apply intro.block}
        <script type="text/javascript" src="/scripts/app.js"></script>
        {squarespace-footers}
    </body>
</html>
