<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="user-scalable=no, width=device-width, initial-scale=1, maximum-scale=1" />
        <meta name="apple-mobile-web-app-capable" content="yes" />
        <meta name="apple-mobile-web-app-status-bar-style" content="black-translucent" />
        <meta name="apple-mobile-web-app-title" content="{.section website}{siteTitle}{.end}" />
        <link href="https://fonts.googleapis.com/css?family=Roboto+Slab:400,700,300,100|Montserrat:400,700" rel="stylesheet" />
        {squarespace-headers}
    </head>
    <body class="-fzero">
        <squarespace:navigation navigationId="navi" template="navi" />
        <div class="hyrule">
            {@|apply header.block}
            <main class="main js-main -wrap -exp" data-content-field="main-content" data-website-id="{website.id}" {.section collection}data-collection-id="{collection.id}" data-collection-title="{.if collection.homepage}{website.siteTitle}{.or}{collection.title} – {website.siteTitle}{.end}"{.end} {.section item}data-item-id="{item.id}" data-item-title="{item.title} – {collection.title} – {website.siteTitle}"{.end}>
                {squarespace.main-content}
            </main>
            {@|apply footer.block}
        </div>
        {@|apply intro.block}
        <script type="text/javascript" src="/scripts/app.js"></script>
        {squarespace-footers}
    </body>
</html>
